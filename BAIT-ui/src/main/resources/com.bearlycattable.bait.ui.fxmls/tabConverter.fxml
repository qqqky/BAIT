          <children>
            <VBox minWidth="568.0">
              <children>
                <VBox alignment="CENTER">
                  <children>
                    <Label alignment="CENTER" text="%label.wifForCompressed" />
                  </children>
                </VBox>
                <TextField fx:id="converterWIFTextFieldResultCompressed" alignment="CENTER" />
              </children>
            </VBox>
          </children>
        </HBox>
        <VBox prefHeight="5.0" />
        <HBox alignment="CENTER">
          <children>
            <Label alignment="CENTER" styleClass="errorInfoMessage" fx:id="converterLabelSuccessErrorResultForWIF" />
          </children>
        </HBox>
        <VBox prefHeight="5.0" prefWidth="1226.0" />
      </children>
    </VBox>
  </children>
</VBox>
